                    "expr": "topk(5,\n  avg by (namespace, deployment) (\n    label_replace(\n      (\n        rate(container_cpu_usage_seconds_total{container!=\"POD\",container!=\"\"}[$__rate_interval])\n        / on(namespace, pod, container)\n          kube_pod_container_resource_requests{resource=\"cpu\"}\n      )\n      * on(namespace, pod) group_left(created_by_name)\n        kube_pod_info{created_by_kind=\"ReplicaSet\"},\n      \"deployment\", \"$1\", \"created_by_name\", \"(.+)-.+\"\n    )\n  )\n)",
